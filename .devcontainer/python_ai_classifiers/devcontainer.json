{
  "name": "ai_classifiers Environment",
  "image": "mcr.microsoft.com/devcontainers/universal:2",
  "workspaceFolder": "/workspaces/askmarvin-dev-docs-playground",
  "postCreateCommand": "cd python/setup && pip install -r ../requirements.txt",
  "extensions": [
    "dev-docs.dev-docs"
  ],
  "customizations": {
    "codespaces": {
      "openFiles": [
        "python/ai_classifiers/ai_classifiers.py"
      ]
    }
  }
}
